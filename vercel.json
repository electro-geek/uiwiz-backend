{
    "version": 2,
    "functions": {
        "backend/index.py": {
            "runtime": "@vercel/python",
            "maxDuration": 60
        }
    },
    "rewrites": [
        {
            "source": "/api/static/(.*)",
            "destination": "/backend/static/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/backend/index.py"
        }
    ]
}