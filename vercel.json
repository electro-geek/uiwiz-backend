{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "pip install -r requirements.txt",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/wsgi.py"
    }
  ],
  "functions": {
    "api/**/*.py": {
      "excludeFiles": "tests/**,__tests__/**,**/*.test.py,**/test_*.py,*.md,.git/**,venv/**,.venv/**,__pycache__/**,*.pyc"
    }
  }
}
