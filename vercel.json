{
    "version": 2,
    "functions": {
        "index.py": {
            "runtime": "@vercel/python",
            "maxDuration": 60
        }
    },
    "routes": [
        {
            "src": "/api/static/(.*)",
            "dest": "/backend/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "index.py"
        }
    ]
}