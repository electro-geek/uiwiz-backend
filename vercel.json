{
    "version": 2,
    "functions": {
        "index.py": {
            "runtime": "@vercel/python"
        }
    },
    "rewrites": [
        {
            "source": "/api/static/(.*)",
            "destination": "/backend/static/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.py"
        }
    ]
}